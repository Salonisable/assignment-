q.20 PRINT ALL ALPHABETS IN UPPERCASE IN DESCENDING ORDER
Declare
str varchar2(20);
ch char(1):='Z';
c char;
i integer;
begin
i := ASCII(ch);
     LOOP   
            IF (i = 64) then
                   exit;
            END IF;
            c := chr(i);
              dbms_output.put_line(c);
            i :=i-1;
       END LOOP;
END;
===============================================================================================================================================================================
Q.21 PRINT ALL APLHABETS FROM a to z IN ASCENDING
set serveroutput on
    Declare
str varchar2(20);
ch char(1):='A';
c char;
i integer;
begin
i := ASCII(ch);
     LOOP   
            IF (i = 91) then
                   exit;
            END IF;
            c := chr(i);
              dbms_output.put_line(c);
            i :=i+1;
       END LOOP;
END;
