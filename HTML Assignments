55 Accept two nos in a component from the text box. Make single button "Calculate". When button is pressed then Addition,Subtraction,Multiplication and division
   should be displayed in the view.
   Ans)
   
import { Component, OnInit } from '@angular/core';

@Component({
  selector: 'app-head55b',
  templateUrl: './head55b.component.html',
  styleUrls: ['./head55b.component.css']
})
export class Head55bComponent implements OnInit {

  constructor() { }

  ngOnInit(): void {
  }
  num1:number;
  num2:number;

  add:number;
  sub:number;
  pro:number;
  div:number;

  getcal()
  {
    this.add= Number(this.num1)+Number(this.num2);
    this.sub= this.num1-this.num2;
    this.pro= this.num1*this.num2;
    this.div= this.num1/this.num2;
  }
}


//head55.component.html

<h2>ASSIGNMENT : 55</h2>

<input [(ngModel)]="num1"><br>
<input [(ngModel)]="num2"><br>
<button (click)="getcal()">CALCULATE</button>
<h2>{{add}}</h2>
<h2>{{sub}}</h2>
<h2>{{pro}}</h2>
<h2>{{div}}</h2>
<hr>
//app.component.html

<h2>ROOT COMPONENT</h2><hr>
<app-head55></app-head55>
<br>
<hr>

