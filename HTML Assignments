56 Create a textbox and a button in the component. Enter any string . If the string length is more than 5 characters Then a message should be displayed on the view
    as "String is valid"
 Ans)
 
import { Component, OnInit } from '@angular/core';

@Component({
  selector: 'app-head56',
  templateUrl: './head56.component.html',
  styleUrls: ['./head56.component.css']
})
export class Head56Component implements OnInit {

  constructor() { }

  ngOnInit(): void {
  }

  str:string;
  str2:string;

  checkleng()
  {
    var st = this.str;
    if(st.length > 5)
    {
      // document.write("STRING IS VALID");
      this.str2="STRING IS VALID";
    }
    else
    {
      this.str2=" ";
    }
  }

}


//head56.component.html

<h2>ASSIGNMENT : 56</h2>

<input [(ngModel)]="str"><br>
<button (click)="checkleng()">CHECK</button>
<h2>{{str2}}</h2>

//app.component.html

<h2>ROOT COMPONENT</h2>
<br><hr>
<app-head56></app-head56>
<br><hr>
