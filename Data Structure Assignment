Q.2 Create a structure pointer which can access the members of the structure Student and print member values.
#include<stdio.h>
#include<string.h>

  struct student
 {
 	int eid;
 	char name[30];
 	char city[20];
 	
 };
 
main()
{
 	  struct employee e[3];
 	  char nm[20];
 	  char cy[20];
	
	int i;
	for(i=0;i<3;i++)
	{
		printf("Enter id : ");
		scanf("%d",&e[i].eid);
		
		fflush(stdin);
		printf("Enter name : ");
		gets(nm);
		strcpy(e[i].name,nm);
		
		fflush(stdin);
		printf("Enter city : ");
		gets(nm);
		strcpy(e[i].city,cy);	
		
		for(i=0;i<3;i++)
		{
			printf("id : %d,name : %s,city : %s\n",e[i].eid,e[i].city);
		}
		
	}

	

	
	
	
}



